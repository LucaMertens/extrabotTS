{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    // "outFile": "logs",
    "outDir": "dist",
    "strict": true,
    // "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    // "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "pretty": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
